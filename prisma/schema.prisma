generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url      = env("DATABASE_URL")
}

model Product {
    barcode String @id
    price   Int
    name    String
    user    User @relation(fields: [userId],references: [id])
    userId  Int
}


model User {
    id        Int      @id @default(autoincrement())
    email     String   @unique
    password  String  
    products  Product[]
}